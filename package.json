    "@types/react-router-native": "^4.2.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/uuid": "^3.4.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-native-web": "^0.5.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
