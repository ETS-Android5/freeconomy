    "react-native-elements": "^1.0.0-beta2",
    "react-native-navigation": "^1.1.397",
    "react-native-oauth": "^2.2.0",
    "react-navigation": "^1.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-native": "^4.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.46",
    "@types/enzyme": "^3.1.9",
